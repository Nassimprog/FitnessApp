<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FitnessApp.Views.ProgressPage"
             xmlns:views="clr-namespace:FitnessApp.Views"
             xmlns:viewModels="clr-namespace:FitnessApp.ViewModels"
             BackgroundColor="#32353B"
             NavigationPage.HasBackButton="False">
    
    <ContentPage.BindingContext>
        <viewModels:ProgressPageViewModel></viewModels:ProgressPageViewModel>
    </ContentPage.BindingContext>

    <ContentPage.Content>
        <StackLayout Margin ="20" HorizontalOptions="CenterAndExpand">


            <Label Text="Progress" FontSize="Title" TextColor="WhiteSmoke"></Label>
            

        </StackLayout>
    </ContentPage.Content>
</ContentPage>